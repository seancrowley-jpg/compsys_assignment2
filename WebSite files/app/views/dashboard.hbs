{{> menu id="dashboard"}}

<h1>
  Data from Sensor
</h1>
<div class="ui two column grid">
  <div class="column">
    <div class="ui segment">
      <h3>
        Temperature
      </h3>
      <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/1271491/charts/1?bgcolor=%23ffffff&color=%23d62020&dynamic=true&results=60&title=Sensor+Temperature&type=line"></iframe>
    </div>
  </div>
  <div class="column">
    <div class="ui segment">
      <h3>
        Humidity
      </h3>
      <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/1271491/charts/2?bgcolor=%23ffffff&color=%23d62020&dynamic=true&max=100&min=0&results=60&title=Sensor+Humidity&type=spline"></iframe>
    </div>
  </div>
</div>
<div class="ui two column grid">
  <div class="column">
    <div class="ui segment">
      <h3>
        Pressure
      </h3>
      <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/1271491/charts/3?bgcolor=%23ffffff&color=%23d62020&dynamic=true&results=60&title=Sensor+Pressure&type=line"></iframe>
    </div>
  </div>
  <div class="column">
    <div class="ui segment">
      <h3>
        Rain-Detection
      </h3>
      <iframe width="450" height="260" style="border: 1px solid #cccccc;" src="https://thingspeak.com/channels/1271491/charts/4?bgcolor=%23ffffff&color=%23d62020&dynamic=true&max=1&min=0&results=60&title=Rain+Detection&type=column"></iframe>
    </div>
  </div>
</div>
  
  
